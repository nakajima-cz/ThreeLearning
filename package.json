    "browser-sync": "^2.26.7",
    "chokidar": "^3.3.1",
    "chokidar-cli": "^2.1.0",
    "gsap": "^2.0.1",
    "node-sass": "^4.13.1",
    "node-sass-globbing": "0.0.23",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webfontloader": "^1.6.28",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "@createjs/easeljs": "^2.0.0-beta.2",
